#include <unistd.h>

void  ft_putchar(char c)
{
  write(1, &c, 1);  
}

void  ft_first_word(char *str)
{
  int i;
  
  i = 0;
  while (str[i])
  {
    if(str[i] == ' ')
    {
      return;
    }
    ft_putchar(str[i]);
    i++;
  }
  ft_putchar('\n');
}

int main(int count, char **value)
{
  if (count == 1)
    return (ft_first_word(value[1]);
  else
    return (0);
}
